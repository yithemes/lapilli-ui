{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
	"build:types": {
	  "dependsOn": ["^build:types"],
	  "outputs": ["dist/**"]
	},
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
