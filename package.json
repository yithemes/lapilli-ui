{
	"name": "yith-monorepo",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "pnpm run build:types && pnpm run build:packages",
		"build:types": "turbo run build:types",
		"build:packages": "node ./bin/packages/build.js",
		"analyze": "pnpm run build:types && pnpm run build:packages",
		"watch": "concurrently \"pnpm run watch:types\" \"pnpm run watch:packages\"",
		"watch:types": "turbo run watch:types",
		"watch:packages": "node ./bin/packages/watch.js",
		"clean": "turbo run clean",
		"release:publish": "lerna publish from-package --dist-tag latest --no-private",
		"release:dry-run": "lerna publish from-package --no-push --no-git-reset --no-git-tag-version --no-private --registry=\"http://verdaccio.leanza.io:4873/\""
	},
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/plugin-transform-react-jsx": "^7.22.5",
		"@babel/plugin-transform-runtime": "^7.22.10",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.5",
		"@babel/runtime": "^7.16.0",
		"@emotion/babel-plugin": "^11.11.0",
		"@types/lodash": "^4.14.194",
		"@types/node": "^18.16.5",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"babel-plugin-optimize-clsx": "^2.6.2",
		"chalk": "4.1.2",
		"concurrently": "^7.6.0",
		"fast-glob": "^3.2.12",
		"lerna": "^7.1.0",
		"lodash": "^4.17.21",
		"make-dir": "^3.1.0",
		"node-watch": "^0.7.3",
		"prettier": "latest",
		"progress": "^2.0.3",
		"rimraf": "^4.4.1",
		"turbo": "latest",
		"worker-farm": "^1.7.0"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.16.0"
}