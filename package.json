{
  "name": "yith-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:only",
    "build:only": "wp-scripts build --progress",
    "build:types": "turbo run build:types",
    "watch": "pnpm run watch:types && pnpm run watch:types",
    "watch:only": "wp-scripts start --progress --watch --mode=development",
    "watch:types": "turbo run watch:types"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.11.0",
    "@wordpress/scripts": "^25.5.1",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.16.0"
}